<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper">  <!-- mapper 에 대한 식별 -->
    <select id="selectSysDate" resultType="java.util.Date"> <!-- 쿼리문 안에 있는 식별자 -->
        SELECT
            CURDATE()
        FROM DUAL
    </select>
</mapper>